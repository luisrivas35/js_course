<div class="container mt-5">
        <h1>Registered Users</h1>
        <div class="table-responsive">
            <table class="table table-bordered">
                <thead class="thead-dark">
                    <tr>
                        <th>ID</th>
                        <th>Name</th>
                        <th>Last Name</th>
                        <th>Gender</th>
                        <th>Age</th>
                        <th>Registry</th>
                    </tr>
                </thead>
                <tbody>
                    
                    {{#each users}}
                    <tr>
                        <td>{{this.id}}</td>
                        <td>{{this.firstName}}</td>
                        <td>{{this.lastName}}</td>
                        <td>{{this.gender}}</td>
                        <td>{{this.age}}</td>
                        <td>{{this.timestamp}}</td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>